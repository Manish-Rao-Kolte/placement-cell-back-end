<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport">
    <title>Home</title>
    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"> -->
</head>
<body>
  <%- include ("navbar") %>
      <ul role="list" class="divide-y divide-gray-100">
        <% for(let student of studentList) { %> 
          <li class="flex-col sm:flex sm:flex-row justify-between sm:justify-evenly gap-x-6 py-5 mx-16">
            <div class="flex min-w-0 gap-x-6 items-center">
              <img class="h-12 w-12 flex-none rounded-full bg-gray-50" src="https://cdn-icons-png.flaticon.com/128/15339/15339256.png" alt="">
              <div class="min-w-0 flex-auto">
                <p class="text-sm font-semibold leading-6 text-gray-900 inline-flex sm:block"> <%=student.fullName %> </p>
                <p class="sm:mt-1 truncate text-sm leading-6 text-gray-500 inline-flex sm:block sm:ml-0 ml-2"> <%=student.email %> </p>
                <p class="sm:mt-1 sm:hidden truncate text-sm leading-6 text-gray-900 inline-flex sm:ml-0 ml-2"> <%=student.status %> </p>
              </div>
            </div>
            <div class=" hidden shrink-0 sm:flex sm:items-end sm:gap-x-4">
              <div>
                <p class="text-sm leading-6 text-gray-900"> Phone- <%=student.phone %> </p>
                <p class="mt-1 text-xs leading-5 text-gray-500"> Batch- <%=student.batch %> </p>
              </div>
              <div>
                <p class="text-sm leading-6 text-gray-900"> College- <%=student.college %> </p>
                <p class="mt-1 text-xs leading-5 text-gray-500"> Status- <%=student.status %> </p>
              </div>
            </div>
          </li>
        <% } %>
      </ul>
      <div class="flex items-center justify-center  py-6">
        <a href="students/registration-form" class="bg-green-400 py-1 px-2 rounded-md text-sm flex text-center">Add Student</a>
      </div>
    <!-- <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script> -->
</body>
</html>